<table>
  <thead>
    <tr>
      <th>Doação</th>
      <th>Id</th>
      <th>Local</th>
      
      
    </tr>
  </thead>
  <tbody>
    {{#each doacoes}}
    <tr>
      <td>{{this.Doacao}}</td>
      <td>{{this.id}}</td>
      <td>{{this.Local}}</td>
      
      <td>
        <a href="/editdoacoes/{{this.id}}">Editar</a>
        <a href="/deletedoacoes/{{this.id}}" onclick="return confirm('Tem certeza?')">Excluir</a>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>
